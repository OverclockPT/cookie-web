{
  "name": "cookie-web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun run start:dev",
    "start:dev": "concurrently --kill-others \"bun run dev:next\" \"bun run dev:proxy\"",
    "dev:next": "next dev --turbo",
    "dev:proxy": "local-ssl-proxy --key local-key.pem --cert local-cert.pem --source 443 --target 3000",
    "build": "next build",
    "start": "concurrently --kill-others \"bun run start:next\" \"bun run start:proxy\"",
    "start:next": "next start",
    "start:proxy": "local-ssl-proxy --key localhost-key.pem --cert localhost.pem --source 3001 --target 3000",
    "lint": "bun eslint .",
    "lint-interactive": "bun eslint-interactive ."
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.3",
    "@t3-oss/env-nextjs": "^0.12.0",
    "appwrite": "^20.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cookies-next": "^6.1.0",
    "lucide-react": "^0.544.0",
    "next": "^15.2.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@tailwindcss/postcss": "^4.0.15",
    "@types/node": "^20.14.10",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.23.0",
    "eslint-config-next": "^15.2.3",
    "local-ssl-proxy": "^2.0.5",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.0.15",
    "tw-animate-css": "^1.3.8",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.27.0"
  },
  "ct3aMetadata": {
    "initVersion": "7.39.3"
  }
}